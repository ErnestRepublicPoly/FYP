            android:fontFamily="@font/leaguespartan_bold"
            android:textSize="20sp" />

        <TextView
            android:id="@+id/textViewBloodPressure"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:text="TextView"
            android:fontFamily="@font/rockwen"
            android:textSize="20sp" />
    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:layout_marginLeft="55dp"
        android:layout_marginTop="20dp">

        <TextView
            android:id="@+id/textViewBP2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginRight="42sp"
            android:text="Diastolic BP:"
            android:fontFamily="@font/leaguespartan_bold"
            android:textSize="20sp" />

        <TextView
            android:id="@+id/textViewBloodPressure2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:text="TextView"
            android:fontFamily="@font/rockwen"
            android:textSize="20sp" />
    </LinearLayout>


</LinearLayout>